{
  "name": "react-express-boiler",
  "version": "1.0.0",
  "description": "Boilerplate for a React-Express application",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RyanWRowland/react-express-boiler.git"
  },
  "author": "Ryan Rowland",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RyanWRowland/react-express-boiler/issues"
  },
  "homepage": "https://github.com/RyanWRowland/react-express-boiler#readme",
  "dependencies": {
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "socket.io": "^2.2.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.10"
  }
}
